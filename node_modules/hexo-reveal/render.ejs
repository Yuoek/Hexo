<div class="slides" style="
  position: relative;
  padding-bottom: 56.25%;
  padding-top: 35px;
  height: 0;
  overflow: hidden;
">
  <iframe src="<%= src %>" width="<%= width %>" height="<%= height %>" frameborder="0" allowfullscreen="" style="
    position: absolute;
    top:0;
    left: 0;
    width: 100%;
    height: 100%;
  "></iframe>
</div>
